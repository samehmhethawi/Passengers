@model IEnumerable<Passengers.ViewModel.TRCHANGE_CAR_LINESVM >



@{
    Layout = null;
}

<head>
    <title>تغيرات الخطوط على مركبة</title>
    <style>
        .table {
            border: 1px solid #ccc;
            font-size: 16px;
            text-align: center;
            width: 100%;
        }

        tr {
            page-break-inside: avoid;
        }
    </style>
</head>

<body>
    <h1 style="text-align:center;">تغيرات الخطوط على مركبة</h1>

    <table cellpadding="8" cellspacing="0" class="table" DIR="auto">
        <thead>
            <tr>
               

                <th style="border: 1px solid; width: auto;">رمز المركبة</th>
                <th style="border: 1px solid; width: auto;">رقم اللوحة وقت التغير</th>
                <th style="border: 1px solid; width: auto;">محافظة المركبة</th>
                <th style="border: 1px solid; width: auto;">فئة المركبة</th>
                <th style="border: 1px solid; width: auto;">نوع المركبة</th>
                <th style="border: 1px solid; width: auto;">اسم الخط</th>

                <th style="border: 1px solid; width: auto;">نوع الخط</th>

                <th style="border: 1px solid; width: auto;">تاريخ التغير</th>

                


            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td style="width: auto; border: 1px solid">@item.CARNB</td>

                <td style="width: auto; border: 1px solid">@item.TABNU</td>
                <td style="width: auto; border: 1px solid">@item.CITYNB</td>
                <td style="width: auto; border: 1px solid">@item.CARREG</td>
                <td style="width: auto; border: 1px solid">@item.CARKIND</td>
                <td style="width: auto; border: 1px solid">@item.LINENAME</td>
                <td style="width: auto; border: 1px solid">@item.LINE_TYPE</td>
                <td style="width: auto; border: 1px solid">@Convert.ToDateTime(item.UDATE).ToString("dd/MM/yyyy")</td>

               


            </tr>
            }
        </tbody>
    </table>
</body>
