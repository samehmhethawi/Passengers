    <script>

        function showLoading() {
            $("#loadingContainer").fadeIn();
        }
        function stopLoading() {
            $("#loadingContainer").fadeOut(250);
        }
        $(document).ajaxStart(function () {
            showLoading();
        });
        $(document).ajaxError(function () {
            stopLoading();
        });
        $(document).ajaxStop(function () {
            stopLoading();
        });
    </script>
